fw.runScript( fw.currentScriptDir + '/lib/lib.js' )

var exportPage = function() {

  fw.saveDocument( fw.getDocumentDOM() )

  if ( !FibHelper.getResourceDirs() ) return false

  FibExporter.exportDOM( fw.getDocumentDOM() )

  return true
}

try {
  exportPage()
} catch(e) {
  alert([ e, e.lineNumber, e.fileName, e.message ])
}
